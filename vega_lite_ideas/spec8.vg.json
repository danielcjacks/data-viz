{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 1300,
  "height": 1300,
  "data": {
    "url": "https://raw.githubusercontent.com/danielcjacks/data-viz/master/datasets/LGA_2020_AUST.json",
    "format": {
      "type": "topojson",
      "feature": "LGA_2020_AUST"
    }
  },
  "title": "private insurance holders",
  "transform": [
    {
      "lookup": "properties.LGA_CODE20",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/danielcjacks/data-viz/master/datasets/private_insurance_holders_number.csv"
        },
        "key": "LGA_2018",
        "fields": [
          "LGA_2018",
          "OBS_VALUE"
        ]
      }
    },
    {
      "lookup": "properties.LGA_CODE20",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/danielcjacks/data-viz/master/datasets/family_income_aggregates.csv"
        },
        "key": "lga_2016",
        "fields": [
          "total_families"
        ]
      }
    },
    {
      "calculate": "datum.OBS_VALUE / datum.total_families",
      "as": "insurance_percentage"
    },
    {
      "filter": "datum.OBS_VALUE / datum.total_families < 8"
    }
  ],
  "layer": [
    {
      "projection": {
        "type": "identity",
        "reflectY": true
      },
      "mark": {
        "type": "geoshape",
        "tooltip": {
          "content": "data"
        }
      },
      "encoding": {
        "color": {
          "field": "insurance_percentage",
          "type": "quantitative"
        }
      }
    }
  ]
}